@model SystemConfiguration
@{
    ViewBag.Title = "询价参数";
}
@section quickButton{
    @{ Html.RenderPartial("_QuickButtonAdmin"); }
}

<h4>@ViewBag.Title</h4>
<hr />
<form id="editForm" name="editForm" role="form" class="form-horizontal" method="post">
    @Html.AntiForgeryToken()


    <div class="form-group">
        <label class="control-label col-sm-2">
            楼盘误差
        </label>
        <div class="col-sm-3">
            <div class="input-group">
                @Html.TextBoxFor(m => m.ProjectDeviationThisMonth, new { @class = "form-control" })
                <span class="input-group-addon">%</span>
            </div>
            @Html.ValidationMessageFor(m => m.ProjectDeviationThisMonth)
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2">
            片区误差
        </label>
        <div class="col-sm-3">
            <div class="input-group">
                @Html.TextBoxFor(m => m.ZoneDeviationThisMonth, new { @class = "form-control" })
                <span class="input-group-addon">%</span>
            </div>
            @Html.ValidationMessageFor(m => m.ZoneDeviationThisMonth)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-primary" value="保存" />
        </div>
    </div>

</form>

@section Scripts {
    <script src="/scripts/iKitchen.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/jqueryval")

}