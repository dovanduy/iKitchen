<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie ie9" lang="en" class="no-js"> <![endif]-->
<!--[if !(IE)]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<head>
    @Html.Partial("_MetaData")
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="KingAdmin - Bootstrap Admin Dashboard Theme">
    <meta name="author" content="The Develovers">
    <!-- CSS -->
    <link href="/assets/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="/assets/css/font-awesome.css" rel="stylesheet" type="text/css">
    <link href="/assets/css/main.css" rel="stylesheet" type="text/css">
    <!-- CSS for demo style switcher. you can remove this -->
    <link href="/demo-style-switcher/assets/css/style-switcher.css" rel="stylesheet" type="text/css">
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/assets/ico/kingadmin-favicon144x144.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/assets/ico/kingadmin-favicon114x114.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/assets/ico/kingadmin-favicon72x72.png">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="/assets/ico/kingadmin-favicon57x57.png">
    <link rel="shortcut icon" href="/assets/ico/favicon.png">
</head>
<body class="dashboard">
    <!-- WRAPPER -->
    <div class="wrapper">
        @Html.Partial("_TopBar")

        <!-- BOTTOM: LEFT NAV AND RIGHT MAIN CONTENT -->
        <div class="bottom">
            <div class="container">
                <div class="row">
                    @Html.Partial("_LeftSideBar")
                    <!-- content-wrapper -->

                    <div class="col-md-10 content-wrapper">
                        <div class="row">
                            <div class="col-md-4 ">
                                @Html.Partial("_BreadCrumb")
                            </div>
                            <div class="col-md-8 ">
                                <div class="top-content">
                                    @RenderSection("quickButton", required: false)
                                </div>
                            </div>
                        </div>
                        @if (TempData["Message"] != null)
                        {
                            <div id="messageDiv" class="alert alert-@TempData["MessageType"]" role="alert">@TempData["Message"]</div>
                            <script type="text/javascript">
                                function hideNotice() {
                                    $("#messageDiv").fadeOut("slow");
                                }
                                setTimeout(hideNotice, 3000);
                            </script>
                        }
                        <!-- main -->
                        <div class="content">
                            @*<div class="main-header">
                                    <h2>@ViewBag.Title</h2>
                                    <em>@ViewBag.SubTitle</em>
                                </div>*@
                            @RenderBody()
                        </div><!-- /main -->
                    </div><!-- /content-wrapper -->
                </div><!-- /row -->
            </div><!-- /container -->
        </div>
        <!-- END BOTTOM: LEFT NAV AND RIGHT MAIN CONTENT -->
        <div class="push-sticky-footer"></div>
    </div><!-- /wrapper -->
    @Html.Partial("_Footer")
    @Html.Partial("_StyleSwitcher")
    @Html.Partial("_ModalPopUp")
    <!-- Javascript -->
    <script src="/assets/js/jquery-2.1.0.min.js"></script>
    <script src="/assets/js/bootstrap.js"></script>
    <script src="/assets/js/modernizr.js"></script>
    @*<script src="/assets/js/bootstrap-tour.custom.js"></script>*@
    <script src="/assets/js/king-common.js"></script>
    <script src="/demo-style-switcher/assets/js/deliswitch.js"></script>
    <script src="/assets/js/stat/jquery.easypiechart.min.js"></script>
    <script src="/assets/js/raphael-2.1.0.min.js"></script>
    <script src="/assets/js/stat/flot/jquery.flot.min.js"></script>
    <script src="/assets/js/stat/flot/jquery.flot.resize.min.js"></script>
    <script src="/assets/js/stat/flot/jquery.flot.time.min.js"></script>
    <script src="/assets/js/stat/flot/jquery.flot.pie.min.js"></script>
    <script src="/assets/js/stat/flot/jquery.flot.tooltip.min.js"></script>
    <script src="/assets/js/jquery.sparkline.min.js"></script>
    <script src="/assets/js/bootstrap-datepicker.js"></script>
    <script src="/assets/js/datatable/jquery.dataTables.min.js"></script>
    <script src="/assets/js/datatable/jquery.dataTables.bootstrap.js"></script>
    @*<script src="/assets/js/jquery.mapael.js"></script>*@
    @*<script src="/assets/js/maps/usa_states.js"></script>*@
    <script src="/assets/js/king-chart-stat.js"></script>
    <script src="/assets/js/king-table.js"></script>
    <script src="/assets/js/king-components.js"></script>
    <script src="/assets/js/jquery.gritter.min.js"></script>
    <script src="/assets/js/king-elements.js"></script>
    <script src="/scripts/SunTzu.js"></script>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(document.body).on('hidden.bs.modal', function () {
            $('#modalView').removeData('bs.modal')
        });
    </script>
</body>
</html>
